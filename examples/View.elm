module View exposing (..)

import Dialog
import Dialog.Bulma as DialogBulma
import Form.Locale as FormLocale
import Form.View as FormView
import Html
import Html.Attributes as HtmlAttributes
import Html.Events as HtmlEvents
import Http.Detailed as HttpDetailed
import Model
import Msg


view : Model.Model -> Html.Html Msg.Msg
view model =
    Html.div [ HtmlAttributes.class "bulma" ]
        [ Html.div
            [ HtmlAttributes.class
                "p-3"
            ]
            []
        , Html.div
            [ HtmlAttributes.class "container is-max-desktop" ]
            [ Html.h1
                [ HtmlAttributes.class "is-size-3 has-text-weight-bold pb-4" ]
                [ Html.text "Create BusinessUnit" ]
            , Html.div
                [ HtmlAttributes.class "box p-5 has-background-white" ]
                [ Html.map Msg.FormMsg (FormView.view model.startTime model.submitted FormLocale.enAU model.form)
                , Html.div [ HtmlAttributes.class "is-flex is-justify-content-space-between px-3" ]
                    [ Html.button
                        [ HtmlAttributes.class "button is-link "
                        , HtmlEvents.onClick Msg.SubmitForm
                        ]
                        [ Html.text "Cancel" ]
                    , Html.button
                        [ HtmlAttributes.class "button is-link "
                        , HtmlEvents.onClick Msg.SubmitForm
                        ]
                        [ Html.text "Create" ]
                    ]
                ]
            ]
        , viewDialog model.dialog { toMsg = Msg.DialogMsg }
        ]


viewDialog : Dialog.Model String msg -> { toMsg : Dialog.Msg -> msg } -> Html.Html msg
viewDialog model { toMsg } =
    DialogBulma.view
        { viewLoadingIndicator = loadingIndicator
        , viewHttpError = viewHttpError
        , toMsg = toMsg
        }
        model


loadingIndicator : Html.Html msg
loadingIndicator =
    Html.figure
        [ HtmlAttributes.class "image" ]
        [ Html.figure [ HtmlAttributes.class "image" ]
            [ Html.img
                [ HtmlAttributes.class "p-5 is-rounded has-background-white"
                , HtmlAttributes.src "data:image/gif;base64,"
                ]
                []
            ]
        ]


viewHttpError : HttpDetailed.Error String -> Html.Html msg
viewHttpError error =
    case error of
        HttpDetailed.BadUrl url ->
            Html.text <| url ++ " is an invalid URL"

        HttpDetailed.Timeout ->
            Html.text <| "The server cannot be reached, timeout occurred"

        HttpDetailed.NetworkError ->
            Html.text <| "Network connection error, the server cannot be reached"

        HttpDetailed.BadStatus { statusText } _ ->
            Html.text <| statusText

        HttpDetailed.BadBody _ _ err ->
            Html.text <| err
